NODE_ENV=test
PORT=5000

# Database
MONGO_URI=mongodb://localhost:27017/booking-test

# JWT
JWT_SECRET=test-jwt-secret-key-change-in-production

# Stripe (Test Mode Keys)
STRIPE_SECRET_KEY=sk_test_your_stripe_test_secret_key
STRIPE_WEBHOOK_SECRET=whsec_test_your_webhook_secret
STRIPE_PUBLIC_KEY=pk_test_your_stripe_test_public_key

# Frontend URL
FRONTEND_URL=http://localhost:5173

# Cloudinary (Optional for tests)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Google OAuth (Optional for tests)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Email (Optional - mock in tests)
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USER=test@example.com
EMAIL_PASSWORD=test_password
EMAIL_FROM=noreply@example.com

# Redis (Optional)
REDIS_URL=redis://localhost:6379

# Rate Limiting (Disabled in test)
RATE_LIMIT_ENABLED=false

# Logging
LOG_LEVEL=error

# Sentry (disabled by default in tests)
SENTRY_DSN=
SENTRY_TRACES_SAMPLE_RATE=0.0
SENTRY_SEND_DEFAULT_PII=false
SENTRY_ENABLE_LOGS=false
